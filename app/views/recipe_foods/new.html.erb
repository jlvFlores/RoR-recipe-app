<div class="recipe-container">
  <h1>NEW FORM</h1> <%# Replace with your form %> 

  <%= form_with(model: [@recipe, @recipe_food], url: recipe_foods_path(@recipe)) do |form| %>
    <div class="field">
      <%= form.label :quantity %>
      <%= form.text_field :quantity %>
    </div>

    <div class="field">
      <%= form.label :food_id %>
      <%= form.collection_select :food_id, Food.all, :id, :name %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
</div>